{
    "name": "pinai4/process-correlation-id-bundle",
    "description": "Provides the ability to track Process Correlation Id and store it in logs",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey P (home)",
            "email": "pinai4@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0",
        "symfony/messenger": "^4.4 || ^5.0 || ^6.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/console": "^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4 || ^5.0 || ^6.0",
        "nyholm/symfony-bundle-test": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Pinai4\\ProcessCorrelationIdBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pinai4\\ProcessCorrelationIdBundle\\Tests\\": "tests/"
        }
    }
}
